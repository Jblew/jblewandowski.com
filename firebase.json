{
  "hosting": [
    {
      "target": "jblew-pl",
      "public": "blog/public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/praktyka/rejestracja/**",
          "run": {
            "serviceId": "rejestracja",
            "region": "europe-central2"
          }
        }
      ]
    }
  ]
}